     let minute = now.getMinutes();\n        let second = now.getSeconds();\n        //hour\n        hour = hour % 12;\n        hour =\n            (hour * Math.PI) / 6 +\n                (minute * Math.PI) / (6 * 60) +\n                (second * Math.PI) / (360 * 60);\n        this.drawHand(ctx, hour, radius * 0.5, radius * 0.07);\n        //minute\n        minute = (minute * Math.PI) / 30 + (second * Math.PI) / (30 * 60);\n        this.drawHand(ctx, minute, radius * 0.8, radius * 0.07);\n    }\n    drawHand(ctx, pos, length, width) {\n        ctx.beginPath();\n        ctx.lineWidth = width;\n        ctx.lineCap = 'round';\n        ctx.moveTo(0, 0);\n        ctx.rotate(pos);\n        ctx.lineTo(0, -length);\n        ctx.stroke();\n        ctx.rotate(-pos);\n    }\n}\nAnalogueWatchComponent.ɵfac = function AnalogueWatchComponent_Factory(t) { return new (t || AnalogueWatchComponent)(); };\nAnalogueWatchComponent.ɵcmp = /*@__PURE__*/ i0.ɵɵdefineComponent({ type: AnalogueWatchComponent, selectors: [[\"yah-analogue-watch\"]], viewQuery: function AnalogueWatchComponent_Query(rf, ctx) {\n        if (rf & 1) {\n            i0.ɵɵviewQuery(_c0, 7);\n        }\n        if (rf & 2) {\n            let _t;\n            i0.ɵɵqueryRefresh(_t = i0.ɵɵloadQuery()) && (ctx.canvasElement = _t.first);\n        }\n    }, decls: 2, vars: 0, consts: [[\"width\", \"512px\", \"height\", \"512px\"], [\"canvasElement\", \"\"]], template: function AnalogueWatchComponent_Template(rf, ctx) {\n        if (rf & 1) {\n            i0.ɵɵelement(0, \"canvas\", 0, 1);\n        }\n    }, styles: [\"canvas[_ngcontent-%COMP%]{animation:fadeEffect 1s;transition:ease-in all}@keyframes fadeEffect{0%{opacity:0}to{opacity:1}}\"], data: { animation: [] } });\n(function () {\n    (typeof ngDevMode === \"undefined\" || ngDevMode) && i0.ɵsetClassMetadata(AnalogueWatchComponent, [{\n            type: Component,\n            args: [{ selector: 'yah-analogue-watch', animations: [], template: \"<canvas #canvasElement w