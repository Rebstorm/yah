{
  "name": "yah",
  "productName": "yah",
  "version": "0.7.0",
  "main": "electron-wrapper.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build-all",
    "test-yah-light": "ng test yah-light --watch=false --browsers=ChromeHeadless",
    "test-yah-cleaning": "ng test yah-cleaning --watch=false --browsers=ChromeHeadless",
    "test-yah-weather": "ng test yah-weather --watch=false --browsers=ChromeHeadless",
    "test-yah-solar": "ng test yah-solar --watch=false --browsers=ChromeHeadless",
    "test-yah-ui": "ng test yah-ui --watch=false --browsers=ChromeHeadless",
    "test-yah": "ng test yah --watch=false --browsers=ChromeHeadless",
    "build-all": "ng b yah-ui && ng b yah-light && ng b yah-weather && ng b yah-cleaning && ng b yah-solar && ng b yah",
    "build-all-prd": "ng b yah-ui && ng b yah-light && ng b yah-weather && ng b yah-cleaning && ng b yah-solar && ng b yah --prod",
    "build-electron": "npm run build-all && tsc projects/electron-wrapper.ts --outDir ./dist",
    "build-electron-prd": "npm run build-all-prd && tsc projects/electron-wrapper.ts --outDir ./dist",
    "start-electron": "electron ./dist/electron-wrapper.js ",
    "start-sw": "http-server -p 8080 -c-1 dist/yah",
    "build-executables-x64": "npm run build-electron && cp package.json ./dist/ && electron-packager ./dist/ yah --platform=linux --arch=x64 --asar --out=./bin/",
    "build-executables-x64-prd": "npm run build-electron-prd && cp package.json ./dist/ && electron-packager ./dist/ yah --platform=linux --arch=x64 --asar --out=./bin/ --overwrite",
    "build-executables-arm": "npm run build-electron && cp package.json ./dist/ && electron-packager ./dist/ yah --platform=linux --arch=arm64 --asar --out=./bin/",
    "build-executables-arm-prd": "npm run build-electron-prd && cp package.json ./dist/ && electron-packager ./dist/ yah --platform=linux --arch=arm64 --asar --out=./bin/ --overwrite"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.0.5",
    "@angular/cdk": "12.0.5",
    "@angular/common": "12.0.5",
    "@angular/compiler": "12.0.5",
    "@angular/core": "12.0.5",
    "@angular/forms": "12.0.5",
    "@angular/platform-browser": "12.0.5",
    "@angular/platform-browser-dynamic": "12.0.5",
    "@angular/router": "12.0.5",
    "@angular/service-worker": "12.0.5",
    "@ngneat/hot-toast": "2.0.2",
    "@ngneat/overview": "1.0.0",
    "@ngx-pwa/local-storage": "11.1.0",
    "ngx-spinner": "11.0.2",
    "process": "0.11.10",
    "rxjs": "7.1.0",
    "tslib": "2.3.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.0.4",
    "@angular/cli": "12.0.4",
    "@angular/compiler-cli": "12.0.5",
    "@types/jasmine": "3.7.7",
    "@types/node": "14.17.3",
    "codelyzer": "6.0.2",
    "electron": "13.1.2",
    "electron-packager": "15.2.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "ng-packagr": "11.2.4",
    "prettier": "2.3.1",
    "protractor": "7.0.0",
    "ts-node": "10.0.0",
    "tslint": "6.1.3",
    "typescript": "4.2.4",
    "http-server": "0.12.3"
  }
}
