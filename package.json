{
  "name": "yah",
  "version": "0.1.1",
  "main": "electron-wrapper.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build-all",
    "test-yah-light": "ng test yah-light --watch=false --browsers=ChromeHeadless",
    "test-yah-cleaning": "ng test yah-cleaning --watch=false --browsers=ChromeHeadless",
    "test-yah-weather": "ng test yah-weather --watch=false --browsers=ChromeHeadless",
    "test-yah": "ng test yah --watch=false --browsers=ChromeHeadless",
    "build-all": "ng b yah-light && ng b yah-weather && ng b yah-cleaning",
    "build-electron": "npm run build-all && npm run build && tsc projects/electron-wrapper.ts --outDir ./dist",
    "start-electron": "npm run build-electron && electron ./dist/main.js ",
    "build-executables": "npm run build-electron && cp package.json ./dist/ && electron-packager ./dist/ yah --platform=linux --arch=x64 --asar --out=./bin/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.0",
    "@angular/cdk": "11.2.0",
    "@angular/common": "11.2.0",
    "@angular/compiler": "11.2.0",
    "@angular/core": "11.2.0",
    "@angular/forms": "11.2.0",
    "@angular/platform-browser": "11.2.0",
    "@angular/platform-browser-dynamic": "11.2.0",
    "@angular/router": "11.2.0",
    "@ngneat/hot-toast": "1.0.8",
    "@ngneat/overview": "1.0.0",
    "@ngx-pwa/local-storage": "11.1.0",
    "ngx-spinner": "10.0.1",
    "node-hue-api": "4.0.10",
    "process": "0.11.10",
    "rxjs": "6.6.3",
    "tslib": "2.1.0",
    "zone.js": "0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1102.0",
    "@angular/cli": "11.2.0",
    "@angular/compiler-cli": "11.2.0",
    "@types/jasmine": "3.6.3",
    "@types/node": "12.20.0",
    "codelyzer": "6.0.1",
    "electron": "11.2.3",
    "electron-packager": "15.2.0",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "karma": "6.1.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "ng-packagr": "11.2.1",
    "prettier": "2.2.1",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "4.1.5"
  }
}
